{
  "name": "faradey/tcpdi",
  "version": "2.0.0",
  "type": "library",
  "description": "TCPDI is a PHP class for importing PDF to use with TCPDF",
  "keywords": [
    "PDF",
    "tcpdi",
    "tcpdi_parser",
    "tcpdf"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Vitalij",
      "email": "rvansp@gmail.com",
      "homepage": "https://extmag.com/en"
    }
  ],
  "require": {
    "php": ">=7.4",
    "tecnickcom/tcpdf": "^6.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "classmap": [
      "fpdf_tpl.php",
      "tcpdi.php",
      "tcpdi_parser.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
